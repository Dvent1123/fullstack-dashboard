n\r\n\r\n    const getUsers = async () =>{\r\n        const parseToken = JSON.parse(token)\r\n        let res = await getAllUsers(parseToken.token)\r\n        setUsers(res.usersArray)\r\n    }\r\n\r\n    const newUserFunction = async (userObj) => {\r\n        socket.on('UserAdded', (result) => {\r\n            const {data, success} = result\r\n            if(!success){\r\n                const errorToast = {\r\n                title: 'Danger',\r\n                description: 'There was an error :(',\r\n                backgroundColor: '#d9534f',\r\n                icon: errorIcon\r\n                }\r\n\r\n                setToast(errorToast)\r\n                toggle()\r\n                toggleToast()            \r\n            }else{\r\n                setUsers([...users, data])\r\n                const successToast = {\r\n                    title: 'Success',\r\n                    description: 'User added!',\r\n                    backgroundColor: '#5cb85c',\r\n                    icon: checkIcon\r\n                }\r\n                setToast(successToast)\r\n                toggle()\r\n                toggleToast()            }   \r\n        })\r\n    }\r\n\r\n    const onSubmit = (e) => {\r\n        e.preventDefault()\r\n        const newUser = {\r\n            username: userName,\r\n            password: password,\r\n            role: role,\r\n            job: job\r\n        }\r\n\r\n        socket.emit('addUser', newUser)\r\n        newUserFunction()\r\n\r\n        setUserName('')\r\n        setPassword('')\r\n        setJob('')\r\n        setRole('User')\r\n    }\r\n\r\n\r\n    useEffect(()=> {\r\n        if(!users){\r\n            getUsers();\r\n        }\r\n    })\r\n\r\n    //renders the users\r\n    const renderUsers = (user) => {\r\n        return (\r\n            <div key={user._id}>\r\n                <UsersContainer user={user} users={users} setUsers={setUsers}/>\r\n                </div>\r\n        )\r\n    }\r\n\r\n    return (\r\n            <section className=\"home-containers\">\r\n                <Nav />\r\n                <div className=\"section-title\">\r\n                    <h1>Users</h1>\r\n                    <button className=\"button-default\" onClick={toggle}><AiFillPlusCircle size={'40px'}/></button>\r\n                </div>\r\n                <UsersModal isShowing={isShown} hide={toggle} onSubmit={onSubmit} \r\n                userName={userName} setUserName={setUserName}\r\n                password={password} setPassword={setPassword}\r\n                role={role} setRole={setRole}\r\n                job={job} setJob={setJob}/>\r\n                <Toast toast={toast} position={'bottom-right'} isShowing={isShownToast} hide={toggleToast}/>\r\n                <section className=\"section-container\">\r\n                        { loading === false ?\r\n                            (<div className=\"users\">\r\n                            {\r\n                                (users && users.length > 0) ? (\r\n                                    users.map(user => {\r\n                                        return renderUsers(user)\r\n                                    })\r\n                                ) :(\r\n                                        <p>No Users found</p>\r\n                                )\r\n                            }\r\n                            </div>) : (\r\n                                <Loading />\r\n                            )\r\n                        }\r\n                    </section>\r\n            </section>\r\n    )\r\n}\r\n\r\nexport default Users\r\n","C:\\Users\\ventu\\Documents\\GitHub\\fullstack-dashboard\\client\\src\\components\\Helpers\\UsersContainer.js",[],"C:\\Users\\ventu\\Documents\\GitHub\\fullstack-dashboard\\client\\src\\components\\Helpers\\TasksContainer.js",[],"C:\\Users\\ventu\\Documents\\GitHub\\fullstack-dashboard\\client\\src\\components\\Helpers\\AssetsContainer.js",[],"C:\\Users\\ventu\\Documents\\GitHub\\fullstack-dashboar